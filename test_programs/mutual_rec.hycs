# describe(is even/odd)
# input(4, 7)
fun isOdd(n: Int): Bool {
    if n == 0 then {
        false
    } else {
        isEven(n - 1)
    }
};

fun isEven(n: Int): Bool {
    if n == 0 then {
        true
    } else {
        isOdd(n - 1)
    }
};
